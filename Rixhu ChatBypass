local function runScript1()
	print(string.char(82, 117, 110, 110, 105, 110, 103, 32, 83, 99, 114, 105, 112, 116, 32, 49, 46, 46, 46))
local _SSX = game:GetService(string.char(83, 111, 117, 110, 100, 83, 101, 114, 118, 105, 99, 101))
local RunService = game:GetService(string.char(82, 117, 110, 83, 101, 114, 118, 105, 99, 101))

local _SSD = Instance.new(string.char(83, 111, 117, 110, 100))
StartupSound.SoundId = string.char(114, 98, 120, 97, 115, 115, 101, 116, 105, 100, 58, 47, 47, 55, 52, 56, 49, 49, 57, 51, 53, 54, 48)
StartupSound.Volume = 1
StartupSound.Parent = SoundService

StartupSound:Play()

local function fadeOutSound(sound, fadeTime)
    local startTime = tick()
    local startVolume = sound.Volume

    local connection
    connection = RunService.Heartbeat:Connect(function()
        local elapsed = tick() - startTime
        local alpha = math.clamp(elapsed / fadeTime, 0, 1)
        sound.Volume = startVolume * (1 - alpha)

        if alpha >= 1 then
            connection:Disconnect()
            sound:Stop()
            sound:Destroy()
        end
    end)
end

StartupSound.Ended:Connect(function()
    fadeOutSound(StartupSound, 2) -- 2 seconds fade out
end)
