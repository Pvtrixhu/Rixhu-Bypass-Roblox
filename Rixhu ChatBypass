    [string.char(90)] = string.char(127487, 32),
    [string.char(122)] = string.char(127487, 32),
    [string.char(48)] = string.char(9450),
    [string.char(49)] = string.char(9312),
    [string.char(50)] = string.char(9313),
    [string.char(51)] = string.char(9314),
    [string.char(52)] = string.char(9315),
    [string.char(53)] = string.char(9316),
    [string.char(54)] = string.char(9317),
    [string.char(55)] = string.char(9318),
    [string.char(56)] = string.char(9319),
    [string.char(57)] = string.char(9320),
    [string.char(32)] = string.char(32)
}

    local function filter(message)
        for search, replacement in pairs(letters) do
            message = replace(message, search, replacement)
        end
        return message
    end
    local function showNotification(title, text)
        StarterGui:SetCore(string.char(83, 101, 110, 100, 78, 111, 116, 105, 102, 105, 99, 97, 116, 105, 111, 110), {Title = title, Text = text, Duration = 5})
    end
    local function sendChat(msg)
    local filteredMessage = game:GetService(string.char(67, 104, 97, 116)):FilterStringForBroadcast(msg, LocalPlayer)
    local tagged = filteredMessage ~= msg
    local keepTextCheckbox = script.Parent.Parent.Parent:FindFirstChild(string.char(75, 101, 101, 112, 84, 101, 120, 116, 67, 104, 101, 99, 107, 98, 111, 120))
    local keepTextTick = keepTextCheckbox and keepTextCheckbox:FindFirstChild(string.char(84, 105, 99, 107))

    if tagged then
        showNotification(string.char(72, 69, 76, 76, 32, 78, 65, 72, 32, 33, 33), string.char(84, 69, 88, 84, 32, 70, 85, 67, 75, 69, 68))
        textBox.Text = string.char()
    else
        if TextChatService.ChatVersion == Enum.ChatVersion.LegacyChatService then
            ReplicatedStorage:FindFirstChild(string.char(68, 101, 102, 97, 117, 108, 116, 67, 104, 97, 116, 83, 121, 115, 116, 101, 109, 67, 104, 97, 116, 69, 118, 101, 110, 116, 115)).SayMessageRequest:FireServer(msg, string.char(65, 108, 108))
        else
            chat:SendAsync(msg)
        end
        
        -- ONLY clear if checkbox is NOT checked
        if keepTextTick and keepTextTick.TextTransparency == 1 then
            textBox.Text = string.char()
        end
    end
end

    local function processText()
        local inputText = textBox.Text
        local convertedText = filter(inputText)
        sendChat(convertedText)
    end
    textButton.MouseButton1Click:Connect(processText)
    textBox.FocusLost:Connect(
        function(enterPressed)
            if enterPressed then
                processText()
            end
        end
    )
end
task.spawn(C_a)
local function C_11()
    local script = G2L[string.char(49, 49)]
    local main = script.Parent.Parent.ImageLabel
    local button = script.Parent
    local function toggleVisibility()
        for _, child in ipairs(main:GetChildren()) do
            if child:IsA(string.char(84, 101, 120, 116, 66, 111, 120)) or child:IsA(string.char(84, 101, 120, 116, 66, 117, 116, 116, 111, 110)) or child:IsA(string.char(84, 101, 120, 116, 76, 97, 98, 101, 108)) then
                child.Visible = not child.Visible
            end
        end
    end
    button.MouseButton1Click:Connect(toggleVisibility)
end
task.spawn(C_11)
local function C_14()
    local script = G2L[string.char(49, 52)]
    local main = script.Parent.Parent -- The Frame
    local button = script.Parent
    local minimized = false
    local tweenService = game:GetService(string.char(84, 119, 101, 101, 110, 83, 101, 114, 118, 105, 99, 101))

    -- Find the special ImageLabel
    local specialImage
    for _, child in ipairs(main:GetDescendants()) do
        if child:IsA(string.char(73, 109, 97, 103, 101, 76, 97, 98, 101, 108)) and child.Image == string.char(114, 98, 120, 97, 115, 115, 101, 116, 105, 100, 58, 47, 47, 49, 51, 53, 57, 51, 57, 48, 57, 50, 57, 52, 54, 50, 51, 51) then
            specialImage = child
            break
        end
    end

    -- Gather all UI elements to hide (except button and frame)
    local contentToToggle = {}
    for _, child in ipairs(main:GetDescendants()) do
        if (child:IsA(string.char(84, 101, 120, 116, 66, 111, 120)) or child:IsA(string.char(84, 101, 120, 116, 66, 117, 116, 116, 111, 110)) or child:IsA(string.char(84, 101, 120, 116, 76, 97, 98, 101, 108))) and child ~= button then
            table.insert(contentToToggle, child)
        end
    end

    -- Save original background color
    local originalColor = main.BackgroundColor3

    button.MouseButton1Click:Connect(function()
        minimized = not minimized

        if minimized then
            -- Hide content
            for _, ui in ipairs(contentToToggle) do
                ui.Visible = false
            end
            if specialImage then
                specialImage.Visible = false
            end

            -- Animate frame smaller and change color
            local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
            local tween = tweenService:Create(main, tweenInfo, {
                Size = UDim2.new(0, 120, 0, 30),
                BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            })
            tween:Play()

        else
            -- Animate back to full size and change color
            local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
            local tween = tweenService:Create(main, tweenInfo, {
                Size = UDim2.new(0, 316, 0, 128),
                BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            })
            tween:Play()

            tween.Completed:Connect(function()
                -- Restore content after animation
                for _, ui in ipairs(contentToToggle) do
                    ui.Visible = true
                end
                if specialImage then
                    specialImage.Visible = true
                end
            end)
        end
    end)
end
task.spawn(C_14)
local function C_18()
    local script = G2L[string.char(49, 56)]
    local button = script.Parent
    local tick = button:WaitForChild(string.char(84, 105, 99, 107))
    local keepText = false -- Start UNCHECKED

    local _TSX = game:GetService(string.char(84, 119, 101, 101, 110, 83, 101, 114, 118, 105, 99, 101))
    local tweenInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)

    local function updateTick()
        local tween = TweenService:Create(tick, tweenInfo, {TextTransparency = keepText and 0 or 1})
        tween:Play()
    end

    updateTick()

    button.MouseButton1Click:Connect(function()
        keepText = not keepText
        updateTick()
    end)

    local sendButtonScript = G2L[string.char(97)] -- assuming your send button is [string.char(97)]
    sendButtonScript.Parent.MouseButton1Click:Connect(function()
        local textBox = sendButtonScript.Parent.Parent.TextBox
        local inputText = textBox.Text
        local convertedText = sendButtonScript.Parent.Parent.TextBox.Parent.Parent.filter(inputText)
        sendButtonScript.Parent.Parent.TextBox.Parent.Parent.sendChat(convertedText)

        -- Only clear text if string.char(107, 101, 101, 112, 84, 101, 120, 116) is FALSE (unchecked)
        if not keepText then
            textBox.Text = string.char()
        end
    end)
end
task.spawn(C_18)

return G2L[string.char(49)], require
end

button1.MouseButton1Click:Connect(function()
	popEffect(button1)
	fadeOutAll()
	runScript1()
end)

button2.MouseButton1Click:Connect(function()
	popEffect(button2)
	fadeOutAll()
	runScript2()
end)
