local function C_3()
    local script = G2L[string.char(51)]
    local UserInputService = game:GetService(string.char(85, 115, 101, 114, 73, 110, 112, 117, 116, 83, 101, 114, 118, 105, 99, 101))
    local gui = script.Parent
    local dragging
    local dragInput
    local dragStart
    local startPos
    local function update(input)
        local delta = input.Position - dragStart
        gui.Position =
            UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
    gui.InputBegan:Connect(
        function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = input.Position
                startPos = gui.Position
                input.Changed:Connect(
                    function()
                        if input.UserInputState == Enum.UserInputState.End then
                            dragging = false
                        end
                    end
                )
            end
        end
    )
    gui.InputChanged:Connect(
        function(input)
            if
                input.UserInputType == Enum.UserInputType.MouseMovement or
                    input.UserInputType == Enum.UserInputType.Touch
             then
                dragInput = input
            end
        end
    )
    UserInputService.InputChanged:Connect(
        function(input)
            if input == dragInput and dragging then
                update(input)
            end
        end
    )
end
task.spawn(C_3)
local function C_a()
    local script = G2L[string.char(97)]
    local Players = game:GetService(string.char(80, 108, 97, 121, 101, 114, 115))
    local LocalPlayer = Players.LocalPlayer
    local TextChatService = game:GetService(string.char(84, 101, 120, 116, 67, 104, 97, 116, 83, 101, 114, 118, 105, 99, 101))
    local ReplicatedStorage = game:GetService(string.char(82, 101, 112, 108, 105, 99, 97, 116, 101, 100, 83, 116, 111, 114, 97, 103, 101))
    local StarterGui = game:GetService(string.char(83, 116, 97, 114, 116, 101, 114, 71, 117, 105))
    local textButton = script.Parent
    local textBox = script.Parent.Parent.TextBox
    local chat = TextChatService.ChatInputBarConfiguration.TargetTextChannel
    local function replace(str, find_str, replace_str)
        local escaped_find_str = find_str:gsub(string.char(91, 37, 45, 37, 94, 37, 36, 37, 40, 37, 41, 37, 37, 37, 46, 37, 91, 37, 93, 37, 42, 37, 43, 37, 45, 37, 63, 93), string.char(37, 37, 37, 48))
        return str:gsub(escaped_find_str, replace_str)
    end
local letters = {
    [string.char(65)] = string.char(127462, 32),
    [string.char(97)] = string.char(127462, 32),
    [string.char(66)] = string.char(127463, 32),
    [string.char(98)] = string.char(127463, 32),
