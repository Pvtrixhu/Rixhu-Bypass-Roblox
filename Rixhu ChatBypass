local asset2 = string.char(114, 98, 120, 97, 115, 115, 101, 116, 105, 100, 58, 47, 47, 49, 48, 48, 53, 53, 52, 54, 57, 56, 54, 57, 52, 54, 49, 48)

local function createAssetButton(imageId)
	local btn = Instance.new(string.char(73, 109, 97, 103, 101, 66, 117, 116, 116, 111, 110))
	btn.Size = UDim2.new(0, 300, 0, 300)
	btn.Position = UDim2.new(0.5, 0, 0.5, 0)
	btn.AnchorPoint = Vector2.new(0.5, 0.5)
	btn.BackgroundTransparency = 1
	btn.ImageTransparency = 1
	btn.Image = imageId
	btn.ScaleType = Enum.ScaleType.Fit
	btn.Parent = container
	return btn
end

local _B1 = createAssetButton(asset1)
local _B2 = createAssetButton(asset2)

-- Fade in blur and buttons slide out + fade in
TweenService:Create(blur, TweenInfo.new(0.5), {Size = 20}):Play()
task.wait(0.3)

local fadeIn = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)

TweenService:Create(button1, fadeIn, {
	ImageTransparency = 0,
	Position = UDim2.new(0.25, 0, 0.5, 0)
}):Play()

TweenService:Create(button2, fadeIn, {
	ImageTransparency = 0,
	Position = UDim2.new(0.75, 0, 0.5, 0)
}):Play()

-- Pop animation (scaled for bigger size)
local function popEffect(btn)
	local grow = TweenService:Create(btn, TweenInfo.new(0.1), {Size = UDim2.new(0, 330, 0, 330)})
	local shrink = TweenService:Create(btn, TweenInfo.new(0.1), {Size = UDim2.new(0, 300, 0, 300)})
	grow:Play()
	grow.Completed:Connect(function()
		shrink:Play()
	end)
end

local function fadeOutAll()
	TweenService:Create(blur, TweenInfo.new(0.5), {Size = 0}):Play()
	TweenService:Create(button1, TweenInfo.new(0.5), {ImageTransparency = 1}):Play()
	TweenService:Create(button2, TweenInfo.new(0.5), {ImageTransparency = 1}):Play()
	task.wait(0.6)
	ScreenGui:Destroy()
	blur:Destroy()
end

