local G2L = {}
G2L[string.char(49)] = Instance.new(string.char(83, 99, 114, 101, 101, 110, 71, 117, 105), game:GetService(string.char(67, 111, 114, 101, 71, 117, 105)));
G2L[string.char(50)] = Instance.new(string.char(70, 114, 97, 109, 101), G2L[string.char(49)])
local _TSX = game:GetService(string.char(84, 119, 101, 101, 110, 83, 101, 114, 118, 105, 99, 101))

-- Set starting state (small and transparent)
G2L[string.char(50)].Size = UDim2.new(0, 0, 0, 0)
G2L[string.char(50)].BackgroundTransparency = 1

-- Create the tween info for the frame
local frameTweenInfo = TweenInfo.new(
    0.8, -- seconds
    Enum.EasingStyle.Back,
    Enum.EasingDirection.Out
)

-- Goal for frame
local frameGoal = {
    Size = UDim2.new(0, 316, 0, 128),
    BackgroundTransparency = 0
}

local frameTween = TweenService:Create(G2L[string.char(50)], frameTweenInfo, frameGoal)
frameTween:Play()

frameTween.Completed:Connect(function()
    for _, child in ipairs(G2L[string.char(50)]:GetChildren()) do
        if child:IsA(string.char(84, 101, 120, 116, 66, 117, 116, 116, 111, 110)) or child:IsA(string.char(84, 101, 120, 116, 76, 97, 98, 101, 108)) then
            -- Set initial transparency
            child.TextTransparency = 1
            child.BackgroundTransparency = 1

            -- Tween the transparency to visible
            local childTweenInfo = TweenInfo.new(
                0.5, -- seconds
                Enum.EasingStyle.Quad,
                Enum.EasingDirection.Out
            )

            local childGoal = {
                TextTransparency = 0,
                BackgroundTransparency = 0
            }

            local childTween = TweenService:Create(child, childTweenInfo, childGoal)
            
            -- Optional: Add small delay between each child for cooler staggered effect
            task.wait(0.1)

            childTween:Play()
        end
    end
end)

